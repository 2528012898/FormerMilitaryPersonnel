<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:background="@color/pageBg"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusable="true"
    android:focusableInTouchMode="true"
    >
    <View
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:id="@+id/v_statusBar"/>
    <ScrollView
        android:scrollbars="none"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:layout_marginBottom="59dp"
                android:background="@color/white"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:layout_marginTop="19dp"
                    android:layout_marginRight="7dp"
                    android:layout_marginLeft="7dp"
                    android:background="@mipmap/home_my_top_bg"
                    android:layout_width="match_parent"
                    android:layout_height="87dp">

                    <ImageView
                        android:src="@mipmap/home_guangrongbang_rv_img"
                        android:layout_marginLeft="16dp"
                        android:layout_width="66dp"
                        android:layout_height="66dp" />
                    <LinearLayout
                        android:layout_marginLeft="7dp"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <View
                            android:layout_weight="29"
                            android:layout_width="0dp"
                            android:layout_height="0dp"/>
                        <TextView
                            android:id="@+id/tv_name"
                            android:textSize="@dimen/fmpOrdinary_26px"
                            android:textColor="@color/white"
                            android:text="名字"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                        <View
                            android:layout_weight="8"
                            android:layout_width="0dp"
                            android:layout_height="0dp"/>
                        <TextView
                            android:id="@+id/tv_tel"
                            android:textSize="@dimen/fmpWeak1_24px"
                            android:textColor="@color/white"
                            android:text="15000000000"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                        <View
                            android:layout_weight="29"
                            android:layout_width="0dp"
                            android:layout_height="0dp"/>
                    </LinearLayout>
                </LinearLayout>
                <!--功能-->
                <LinearLayout
                    android:paddingLeft="25dp"
                    android:gravity="center_vertical"
                    android:layout_marginTop="19dp"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">
                    <ImageView
                        android:src="@mipmap/home_my_wodeyuyue"
                        android:layout_width="21dp"
                        android:layout_height="21dp" />
                    <TextView
                        android:layout_marginLeft="18dp"
                        android:textSize="@dimen/fmpImportant_30px"
                        android:textColor="#4B4B4B"
                        android:text="我的预约"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#E6E6E6"/>
                <!--我的困难帮扶-->
                <LinearLayout
                    android:id="@+id/tv_wodekunnanbangfu"
                    android:paddingLeft="25dp"
                    android:gravity="center_vertical"
                    android:layout_marginTop="0dp"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">
                    <ImageView
                        android:src="@mipmap/home_my_wodekunnanbangfu"
                        android:layout_width="21dp"
                        android:layout_height="21dp" />
                    <TextView
                        android:layout_marginLeft="18dp"
                        android:textSize="@dimen/fmpImportant_30px"
                        android:textColor="#4B4B4B"
                        android:text="我的困难帮扶"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#E6E6E6"/>
                <!--我的津贴-->
                <LinearLayout
                    android:paddingLeft="25dp"
                    android:gravity="center_vertical"
                    android:layout_marginTop="0dp"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">
                    <ImageView
                        android:src="@mipmap/home_my_wodejintie"
                        android:layout_width="21dp"
                        android:layout_height="21dp" />
                    <TextView
                        android:layout_marginLeft="18dp"
                        android:textSize="@dimen/fmpImportant_30px"
                        android:textColor="#4B4B4B"
                        android:text="我的津贴"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#E6E6E6"/>
                <!--个人资料-->
                <LinearLayout
                    android:paddingLeft="25dp"
                    android:gravity="center_vertical"
                    android:layout_marginTop="0dp"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">
                    <ImageView
                        android:src="@mipmap/home_my_gerenziliao"
                        android:layout_width="21dp"
                        android:layout_height="21dp" />
                    <TextView
                        android:layout_marginLeft="18dp"
                        android:textSize="@dimen/fmpImportant_30px"
                        android:textColor="#4B4B4B"
                        android:text="个人资料"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#E6E6E6"/>
                <!--培训记录-->
                <LinearLayout
                    android:paddingLeft="25dp"
                    android:gravity="center_vertical"
                    android:layout_marginTop="0dp"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">
                    <ImageView
                        android:src="@mipmap/home_my_peixunjilu"
                        android:layout_width="21dp"
                        android:layout_height="21dp" />
                    <TextView
                        android:layout_marginLeft="18dp"
                        android:textSize="@dimen/fmpImportant_30px"
                        android:textColor="#4B4B4B"
                        android:text="培训记录"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#E6E6E6"/>
                <!--就业记录-->
                <LinearLayout
                    android:paddingLeft="25dp"
                    android:gravity="center_vertical"
                    android:layout_marginTop="0dp"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">
                    <ImageView
                        android:src="@mipmap/home_my_jiuyejilu"
                        android:layout_width="21dp"
                        android:layout_height="21dp" />
                    <TextView
                        android:layout_marginLeft="18dp"
                        android:textSize="@dimen/fmpImportant_30px"
                        android:textColor="#4B4B4B"
                        android:text="就业记录"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#E6E6E6"/>
                <!--在线信访-->
                <LinearLayout
                    android:paddingLeft="25dp"
                    android:gravity="center_vertical"
                    android:layout_marginTop="0dp"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">
                    <ImageView
                        android:src="@mipmap/home_my_zaixianxinfang"
                        android:layout_width="21dp"
                        android:layout_height="21dp" />
                    <TextView
                        android:layout_marginLeft="18dp"
                        android:textSize="@dimen/fmpImportant_30px"
                        android:textColor="#4B4B4B"
                        android:text="在线信访"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#E6E6E6"/>
                <!--专属服务-->
                <LinearLayout
                    android:paddingLeft="25dp"
                    android:gravity="center_vertical"
                    android:layout_marginTop="0dp"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp">
                    <ImageView
                        android:src="@mipmap/home_my_zhuanshufuwu"
                        android:layout_width="21dp"
                        android:layout_height="21dp" />
                    <TextView
                        android:layout_marginLeft="18dp"
                        android:textSize="@dimen/fmpImportant_30px"
                        android:textColor="#4B4B4B"
                        android:text="专属服务"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>

            </LinearLayout>



        </LinearLayout>

    </ScrollView>

</LinearLayout>