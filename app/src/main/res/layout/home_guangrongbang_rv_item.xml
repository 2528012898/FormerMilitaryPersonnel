    <TextView
        android:id="@+id/tv2"
        android:layout_marginTop="5dp"
        android:text="标题"
        android:textSize="@dimen/fmpWeak3_20px"
        android:textColor="#4B4B4B"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="13dp"/>
</LinearLayout>